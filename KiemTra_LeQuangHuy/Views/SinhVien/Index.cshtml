@model IEnumerable<KiemTra_LeQuangHuy.Models.SinhVien>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml"; }
<h2>Index</h2>
<table border="1px">
    <tr>
        <th>Mã SV</th>
        <th>Họ tên</th>
        <th>Giới tính</th>
        <th>Ngày sinh</th>
        <th>Hình</th>
        <th>Mã ngành</th>
        <th></th>
    </tr> @foreach (var item in Model)
    {
        <tr>
            <td>@item.MaSV</td>
            <td>@item.HoTen</td>
            <td>@item.GioiTinh</td>
            <td>@item.NgaySinh</td>
            <td>
                <img src="@item.Hinh" width="200" title="images" />
            </td>
            <td>@item.MaNganh</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MaSV }) |
                @Html.ActionLink("Detail", "Detail", new { id = item.MaSV }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MaSV })
            </td>
        </tr>
    }
</table> @Html.ActionLink("Create", "Create", "SinhVien")